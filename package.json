{
  "name": "observable-redux-json-api",
  "version": "1.0.0",
  "main": "lib/jsonapi.js",
  "license": "MIT",
  "keywords": ["observable", "redux", "json", "api", "jsonapi"],
  "scripts": {
    "build": "rimraf lib && tsc --project config/build-tsconfig.json",
    "tdd": "jest --config jest.json --watch",
    "test": "jest --config jest.json",
    "test:nocache": "jest --config jest.json --no-cache",
    "test:update": "jest --config jest.json -u",
    "lint": "tslint --project tsconfig.json --type-check",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "@types/pluralize": "^0.0.27",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "nock": "^9.0.22",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.3",
    "ts-jest": "^21.0.1",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.5.3",
    "xmlhttprequest": "^1.8.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "lodash": "^4.17.4",
    "object-path-immutable": "^0.5.2",
    "pluralize": "^7.0.0",
    "redux-actions": "^2.2.1"
  },
  "peerDependencies": {
    "redux-thunk": ">=1.0.0",
    "rxjs": "^5.4.3"
  },
  "lint-staged": {
    "**/*.json": ["prettier --write", "git add"],
    "**/*.ts": [
      "tslint --project tsconfig.json --config tslint-prettify.json --type-check --fix",
      "prettier --write",
      "git add"
    ]
  }
}
