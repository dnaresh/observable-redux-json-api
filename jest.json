{
  "automock": false,
  "moduleFileExtensions": ["ts", "js"],
  "transform": {
    ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
  "notify": false,
  "verbose": true,
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  }
}
